	</select>
	
	
	<!-- 음식 양 / amount -->
	<select id="amount01" resultType="Integer">
		select count(*) from st_feedback where amount='매우만족' and parent_email=#{parent_email} and b_month=#{month} 
	</select>
	
	<select id="amount02" resultType="Integer">
