	</select>
	
	
	<!-- 음식 간 / taste -->
	<select id="taste01" resultType="Integer">
		select count(*) from st_feedback where taste='적당하다' and parent_email=#{parent_email} and b_month=#{month}
	</select>
	
	<select id="taste02" resultType="Integer">
